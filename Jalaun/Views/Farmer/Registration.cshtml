@model DAL.ViewModel.RegisterViewModel
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Farmer Registration</title>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <script src="~/js/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/app.min.css" rel="stylesheet" type="text/css" />
    <script src="~/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/app.js"></script>
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/jquery-ui.min.js"></script>


    <style>
        body {
            background: linear-gradient(135deg, #e8f5e9, #f1f8f4);
            font-family: "Segoe UI", sans-serif;
        }

        .form-wrapper {
            background: #fff;
            padding: 35px;
            border-radius: 18px;
            max-width: 1200px;
            margin: 40px auto;
            box-shadow: 0 18px 40px rgba(0,0,0,0.12);
        }

        /* ===== STEPPER ===== */
        .stepper {
            display: flex;
            justify-content: space-between;
            margin-bottom: 40px;
            position: relative;
        }

            .stepper::before {
                content: "";
                position: absolute;
                top: 26px;
                left: 0;
                right: 0;
                height: 4px;
                background: #e0e0e0;
            }

        .step {
            text-align: center;
            width: 50%;
            position: relative;
            z-index: 1;
        }

            .step .circle {
                width: 52px;
                height: 52px;
                border-radius: 50%;
                background: #e0e0e0;
                color: #777;
                display: flex;
                align-items: center;
                justify-content: center;
                font-size: 22px;
                margin: auto;
                transition: all 0.3s ease;
            }

            .step.active .circle {
                background: #2e7d32;
                color: #fff;
                box-shadow: 0 0 0 8px rgba(46,125,50,0.2);
            }

            .step.completed .circle {
                background: #1b5e20;
                color: #fff;
            }

        .step-title {
            margin-top: 10px;
            font-size: 14px;
            font-weight: 600;
        }

        /* ===== WIZARD ===== */
        .wizard-step {
            display: none;
            animation: fadeIn 0.4s ease-in-out;
        }

            .wizard-step.active {
                display: block;
            }

        @@keyframes fadeIn {
            from {
                opacity: 0;
                transform: translateY(15px);
            }

            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        .section-card {
            background: #f9fbfa;
            padding: 25px;
            border-radius: 14px;
            border-left: 6px solid #2e7d32;
            margin-bottom: 25px;
        }

            .section-card h6 {
                font-weight: 700;
                color: #2e7d32;
                margin-bottom: 18px;
            }

        .btn-success {
            background: #2e7d32;
            border: none;
            padding: 10px 32px;
            border-radius: 30px;
        }

        .btn-secondary {
            border-radius: 30px;
            padding: 10px 32px;
        }

        /* toaster */
        #toast {
            visibility: hidden;
            min-width: 280px;
            background-color: #28a745;
            color: white;
            padding: 15px 20px;
            border-radius: 5px;
            position: fixed;
            top: 20px;
            right: 20px;
            z-index: 9999;
            font-size: 14px;
            box-shadow: 0 4px 8px rgba(0,0,0,0.2);
        }

            #toast.show {
                visibility: visible;
                animation: slideIn 0.5s, fadeout 0.5s 3s;
            }

        @@keyframes slideIn {
            from {
                opacity: 0;
                transform: translateX(100%);
            }

            to {
                opacity: 1;
                transform: translateX(0);
            }
        }

        @@keyframes fadeout {
            from {
                opacity: 1;
            }

            to {
                opacity: 0;
            }
        }

        /*   OTP model */
        .otp-boxes {
            display: flex;
            justify-content: center;
            gap: 10px;
            margin-top: 15px;
        }

        .otp-input {
            width: 40px;
            height: 40px;
            font-size: 20px;
            text-align: center;
            border: 2px solid #ccc;
            border-radius: 10px;
            background: #f7f7f7;
            transition: all 0.2s ease-in-out;
        }

            .otp-input:focus {
                border-color: #28a745;
                box-shadow: 0 0 8px rgba(40, 167, 69, 0.3);
                background: #fff;
                outline: none;
            }

        #otpModal .btn-success {
            width: 100%;
            font-size: 16px;
            padding: 10px 0;
            border-radius: 8px;
        }

        .otp-resend {
            margin-top: 12px;
            font-size: 0.9rem;
            color: #555;
        }

            .otp-resend a {
                color: #007bff;
                font-weight: bold;
                margin-left: 5px;
                text-decoration: none;
            }

                .otp-resend a:hover {
                    text-decoration: underline;
                }

        #otp-message {
            font-size: 0.9rem;
        }

    </style>
</head>

<body>

    <div class="container">
        <div class="form-wrapper">

            <!-- ================= STEPPER ================= -->
            <div class="stepper">
                <div class="step active" data-step="1">
                    <div class="circle">1</div>
                    <div class="step-title">एग्रीस्टैक विवरण</div>
                </div>

                <div class="step" data-step="2">
                    <div class="circle">2</div>
                    <div class="step-title">पत्राचार पता</div>
                </div>
                <div class="step" data-step="3">
                    <div class="circle">3</div>
                    <div class="step-title">भूमि विवरण</div>
                </div>
                <div class="step" data-step="4">
                    <div class="circle">4</div>
                    <div class="step-title">सहमति एवं अंतिम सबमिशन</div>
                </div>
            </div>
            <div id="toast"> </div>
            <form asp-action="Registration" asp-controller="Farmer" method="post" asp-antiforgery="true" id="registrationForm">
                @*     <input type="hidden" name="tahsilId" id="tahsilHidden" />
                <input type="hidden" name="tahsilAgriStack" id="tahsilAgriStackHidden" /> *@
                <input type="hidden" id="IsOtpVerified" name="IsOtpVerified" value="false" />
                <!-- ================= STEP 1 ================= -->
                <div class="wizard-step active" data-step="1">

                    <div class="section-card">
                        <div class="row g-3">
                            <div class="col-md-3">
                                <label for="example-text-input" class="col-sm-12 col-form-label">
                                    एग्री स्टैकआईडी<i class="text-danger">*</i>
                                </label>
                                <div class="col-sm-12">
                                    <input class="form-control" asp-for="FarmerId" type="text" id="FarmerId" required>
                                    <span asp-validation-for="FarmerId"></span>
                                </div>
                            </div>

                            <div class="col-md-1">
                                <button class="btn btn-info" style="margin-top:38px;box-sizing:border-box;" onclick="SearchFarmerById()">Validate</button>
                            </div>
                        </div>
                        <br />
                        <div class="row g-3">
                            <div class="col-md-6">
                                <label>किसान का नाम<span style="color:red">*</span></label>
                                <input asp-for="FarmerName" readonly class="form-control" required />
                            </div>
                            <div class="col-md-6">
                                <label>मोबाइल नंबर<span style="color:red">*</span></label>
                                <input asp-for="MobileNumberAgriStack" readonly class="form-control" required />
                            </div>
                            <div class="col-md-4">
                                <label>ईमेल</label>
                                <input asp-for="Email" class="form-control" />
                            </div>
                            <div class="col-md-4">
                                <label>लिंग<span style="color:red">*</span></label>
                                <input asp-for="Gender" class="form-control" readonly />
                            </div>
                            <div class="col-md-4">
                                <label>जन्म तिथि<span style="color:red">*</span></label>
                                <input asp-for="DOB" class="form-control" id="DOB" readonly />
                            </div>
                            <div class="col-md-4">
                                <label>
                                    आधार नंबर<span style="color:red">*</span>
                                </label>
                                <input asp-for="AadharCardNo" class="form-control" readonly />
                            </div>
                            <div class="col-md-4">
                                <label>पिता / पति का नाम<span style="color:red">*</span></label>
                                <input asp-for="FatherHusbandName" readonly class="form-control" />
                            </div>

                            <div class="col-md-4">
                                <label>राज्य</label>
                                <input asp-for="State" value="उत्तर प्रदेश" readonly class="form-control" />
                            </div>
                            <div class="col-md-4">
                                <label>ज़िला</label>
                                <input asp-for="District" value="जालौन" readonly class="form-control" />
                            </div>
                            <div class="col-md-4">
                                <label>तहसील<span style="color:red">*</span></label>
                                <select asp-for="tahsilAgriStack"
                                        asp-items="@(new SelectList(Model.TehsilList, "Id", "TehsilName"))"
                                        class="form-select" readonly>
                                    <option value="">-- चयन करें --</option>
                                </select>
                            </div>

                            <div class="col-md-4">
                                <label>पिनकोड<span style="color:red">*</span></label>
                                <input asp-for="pincodeAgriStack" class="form-control" readonly />
                            </div>

                        </div>

                    </div>

                    <div class="text-end">
                        <button type="button" class="btn btn-success nextBtn">
                            Next ➜ पत्राचार पता
                        </button>
                    </div>
                </div>

                <!-- ================= STEP 2 ================= -->
                <div class="wizard-step active" data-step="2">

                    <div class="section-card">
                        <div class="row g-3">
                            <div class="col-md-4">
                                <label>राज्य<span style="color:red">*</span></label>
                                <input asp-for="State" value="उत्तर प्रदेश" readonly class="form-control" />
                            </div>
                            <div class="col-md-4">
                                <label>ज़िला<span style="color:red">*</span></label>
                                <input asp-for="District" value="जालौन" readonly class="form-control" />
                            </div>

                            <div class="col-md-4">
                                <label>तहसील<span style="color:red">*</span></label>
                                <select asp-for="tahsilId"
                                        asp-items="@(new SelectList(Model.TehsilList, "Id", "TehsilName"))"
                                        class="form-select blur-validate" id="tahsilId" required>
                                    <option value="">-- चयन करें --</option>
                                </select>
                            </div>

                            <div class="col-md-4">
                                <label class="form-label mb-1">गांव<span style="color:red">*</span></label>
                                <select asp-for="village" class="form-select blur-validate" id="VillageTypeDropdown" required></select>
                            </div>

                            <div class="col-md-4">
                                <label>पिनकोड<span style="color:red">*</span></label>
                                <input asp-for="pincode"
                                       class="form-control blur-validate"
                                       maxlength="6"
                                       data-type="pincode"
                                       required />
                            </div>

                            <div class="col-md-4">
                                <label>मोबाइल नंबर<span style="color:red">*</span></label>
                                <input asp-for="MobileNumber"
                                       class="form-control blur-validate"
                                       maxlength="10"
                                       data-type="mobile"
                                       required />
                            </div>


                            <div class="col-md-12">
                                <label>पूरा पता</label>
                                <input asp-for="address" class="form-control" />
                            </div>
                        </div>
                    </div>

                    <div class="section-card">
                        <h5 class="mb-3 text-success fw-semibold">
                            🔐 पासवर्ड प्रबंधन
                        </h5>

                        <div class="row g-3">
                            <!-- Password Note -->
                            <div class="col-md-12">
                                <div class="alert alert-info mt-2 py-2">
                                    <small>
                                        <strong>नोट:</strong> पासवर्ड कम से कम <b>8 अक्षरों</b> का होना चाहिए।
                                        इसमें <b>एक Capital Letter</b>, <b>एक Small Letter</b>,
                                        <b>एक Number</b> और <b>एक Special Character</b>
                                        (जैसे: <code>@@ $ ! % * ? & /</code>) होना अनिवार्य है।
                                    </small>
                                </div>
                            </div>
                            <!-- Password -->
                            <div class="col-md-4">
                                <label>पासवर्ड <span style="color:red">*</span></label>
                                <div class="input-group">
                                    <input asp-for="Password"
                                           type="password"
                                           class="form-control blur-validate-password"
                                           data-type="password"
                                           maxlength="20"
                                           id="Password"
                                           required />
                                    <button class="btn btn-outline-secondary toggle-password" type="button" data-target="#Password">
                                        <i class="fa fa-eye"></i>
                                    </button>
                                </div>
                                <span class="invalid-feedback"></span>
                            </div>

                            <!-- Confirm Password -->
                            <div class="col-md-4">
                                <label>कन्फर्म पासवर्ड <span style="color:red">*</span></label>
                                <div class="input-group">
                                    <input asp-for="ConfirmPassword"
                                           type="password"
                                           class="form-control blur-validate-password"
                                           data-type="confirm-password"
                                           maxlength="20"
                                           id="ConfirmPassword"
                                           required />
                                    <button class="btn btn-outline-secondary toggle-password" type="button" data-target="#ConfirmPassword">
                                        <i class="fa fa-eye"></i>
                                    </button>
                                </div>
                                <span class="invalid-feedback"></span>
                            </div>



                        </div>
                    </div>


                    <div class="text-end">
                        <div class="d-flex justify-content-between">
                            <button type="button" class="btn btn-secondary prevBtn">
                                ⬅ Previous
                            </button>

                            <button type="button" class="btn btn-success nextBtn btn-step2">
                                Next ➜ भूमि विवरण
                            </button>
                        </div>
                    </div>
                </div>

                <!-- ================= STEP 3================= -->
                <div class="wizard-step active" data-step="3">

                    <div class="section-card">
                        <h6>भूमि विवरण</h6>
                        <div class="row g-3">
                            <div class="col-md-4">
                                <label>एग्री स्टैकआईडी<span style="color:red">*</span></label>
                                <input id="agriStackId" readonly class="form-control" />
                            </div>
                            <div class="col-md-4">
                                <label>किसान का नाम(एग्री स्टैक)<span style="color:red">*</span></label>
                                <input asp-for="FarmerNameForLand" readonly class="form-control" required />
                            </div>
                            <div class="col-md-4">
                                <label>मोबाइल नंबर<span style="color:red">*</span></label>
                                <input asp-for="MobileNumberAgriStackForLand" class="form-control" required />
                            </div>
                            <div class="col-md-4">
                                <label class="form-label mb-1">राज्य<span style="color:red">*</span></label>
                                <input asp-for="LandState" value="उत्तर प्रदेश" readonly class="form-control">
                            </div>

                            <div class="col-md-4">
                                <label class="form-label mb-1">ज़िला<span style="color:red">*</span></label>
                                <input asp-for="LandDistrict" value="जालौन" readonly class="form-control">
                            </div>

                            <div class="col-md-4">
                                <label class="form-label mb-1">तहसील<span style="color:red">*</span></label>
                                <select asp-for="Landtahsil"
                                        asp-items="@(new SelectList(Model.TehsilList, "Id", "TehsilName"))"
                                        class="form-select" id="LandTahsilDropdown" required>
                                    <option value="">-- चयन करें --</option>
                                </select>
                            </div>

                            @*  <div class="col-md-3">
                                <label class="form-label mb-1">क्षेत्र का प्रकार</label>
                                <select asp-for="AreaType" class="form-select" id="LandAreaTypeDropdown"></select>
                            </div> *@

                            <div class="col-md-4">
                                <label class="form-label mb-1">गांव<span style="color:red">*</span></label>
                                <select asp-for="LandVillage" class="form-select" id="LandVillageTypeDropdown" required></select>
                            </div>

                            <div class="col-md-4">
                                <label>खसरा संख्या</label>
                                <input asp-for="LandRecordNumber" class="form-control" onblur="SearchKhasraNo()" id="LandRecordNumber" />
                            </div>
                            <div class="col-md-4">
                                <label> किसान का नाम </label>
                                <input asp-for="FarmerNameLandKhasra" class="form-control" />
                            </div>
                            @*   <div class="col-md-4">
                                <label>एग्रीस्टैक कुल रकबा</label>
                                <input asp-for="TotalAreaAgristack" class="form-control" />
                            </div> *@
                            <div class="col-md-4">
                                <label>कुल रकबा (हे.)</label>
                                <input asp-for="LandTotalArea" class="form-control" readonly />
                            </div>
                            <div class="col-md-4">
                                <label>किसान की हिस्सेदारी</label>
                                <input asp-for="FarmerShare" class="form-control" readonly />
                            </div>

                            <div class="col-md-4">
                                <button type="button" class="btn btn-success" id="AddLandBtn" style="margin-top:27px;">
                                    ➕ Add
                                </button>
                            </div>
                        </div>

                        <div class="row g-3">
                            <div class="section-card mt-4">
                                <h6>जोड़ी गई भूमि विवरण सूची</h6>

                                <table class="table table-bordered table-striped" id="LandGrid">
                                    <thead class="table-success">
                                        <tr>
                                            <th>तहसील</th>
                                            @*    <th>क्षेत्र प्रकार</th> *@
                                            <th>गांव</th>
                                            <th>खसरा</th>
                                            @*  <th>एग्रीस्टैक रकबा</th> *@
                                            <th>कुल रकबा</th>
                                            @* <th>बोया गया</th> *@
                                            <th>हिस्सेदारी</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    </tbody>
                                    <tfoot class="table-secondary fw-bold text-center">
                                        <tr>
                                            <th colspan="2">कुल</th>
                                            <th id="TotalKhasra">0</th>
                                            <th id="TotalArea">0</th>
                                            <th id="TotalShare">0</th>
                                            <th></th>
                                        </tr>
                                    </tfoot>

                                </table>
                            </div>
                            <div id="LandHiddenFields"></div>
                        </div>

                    </div>

                    <div class="text-end">
                        <div class="d-flex justify-content-between">
                            <button type="button" class="btn btn-secondary prevBtn">
                                ⬅ Previous
                            </button>
                            <button type="button" class="btn btn-success nextBtn">
                                Next ➜ सहमति एवं अंतिम सबमिशन
                            </button>
                        </div>
                    </div>
                </div>

                <!-- ================= STEP 4 ================= -->
                <div class="wizard-step" data-step="4">

                    <div class="section-card">
                        <div class="row g-3">
                            <!-- ================= DECLARATION ================= -->
                            <div class="row mt-4">
                                <div class="col-12">
                                    <div class="card border-success shadow-sm rounded-4">
                                        <div class="card-header bg-success text-white rounded-top-4">
                                            <h6 class="mb-0">
                                                <i class="bi bi-file-earmark-text"></i> घोषणा (Declaration)
                                            </h6>
                                        </div>

                                        <div class="card-body">
                                            <p class="mb-3 text-justify" style="font-size:14px;">
                                                मैं यह घोषणा करता/करती हूँ कि मेरे द्वारा किसान पंजीकरण प्रपत्र एवं
                                                एग्रीस्टैक / भूमि विवरण में दी गई समस्त जानकारी मेरी पूर्ण जानकारी
                                                एवं विश्वास के अनुसार सत्य एवं सही है। किसी भी प्रकार की गलत, असत्य
                                                अथवा भ्रामक जानकारी पाए जाने पर मेरा पंजीकरण निरस्त किया जा सकता है
                                                तथा इसके लिए मैं स्वयं उत्तरदायी रहूँगा/रहूँगी।
                                                मैं संबंधित विभाग को मेरे द्वारा दी गई जानकारी के सत्यापन हेतु
                                                अधिकृत करता/करती हूँ।
                                            </p>

                                            <div class="form-check">
                                                <input class="form-check-input" type="checkbox"
                                                       id="DeclarationAccepted" required>
                                                <label class="form-check-label fw-semibold"
                                                       for="DeclarationAccepted">
                                                    मैं उपरोक्त दी गई समस्त जानकारी को सत्य एवं सही घोषित करता/करती हूँ।
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>



                        <div class="d-flex justify-content-between">
                            <button type="button" class="btn btn-secondary prevBtn">
                                ⬅ Previous
                            </button>
                            @* <button type="submit" class="btn btn-success">
                                Final Submit
                            </button> *@
                            <button type="button" class="btn btn-primary" onclick="showPreview()">
                                👁️ Preview
                            </button>


                            @*                             <button type="submit" class="btn btn-success d-none" id="FinalSubmitBtn">
                                ✔ Confirm & Submit
                            </button> *@
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>

    <!--  PREVIEW MODAL -->
    <div class="modal fade" id="previewModal" tabindex="-1">
        <div class="modal-dialog modal-xl modal-dialog-scrollable">
            <div class="modal-content">

                <div class="modal-header bg-primary text-white">
                    <h5 class="modal-title">👁️ पंजीकरण पूर्वावलोकन (Preview)</h5>
                    <button type="button" class="btn-close btn-close-white" data-bs-dismiss="modal"></button>
                </div>

                <div class="modal-body">

                    <!-- AGRISTACK DETAILS -->
                    <h6 class="text-success border-bottom pb-1">एग्रीस्टैक विवरण</h6>
                    <table class="table table-sm table-bordered">
                        <tr><th>किसान आईडी</th><td id="pvFarmerId"></td></tr>
                        <tr><th>किसान नाम</th><td id="pvFarmerName"></td></tr>
                        <tr><th>मोबाइल</th><td id="pvMobileAgri"></td></tr>
                        <tr><th>लिंग</th><td id="pvGenderAgri"></td></tr>
                        <tr><th>आधार</th><td id="pvAadhar"></td></tr>
                        <tr><th>जन्म तिथि</th><td id="pvDOB"></td></tr>
                    </table>

                    <!-- ADDRESS -->
                    <h6 class="text-success border-bottom pb-1 mt-3">पत्राचार पता</h6>
                    <table class="table table-sm table-bordered">
                        <tr><th>राज्य</th><td id="pvState"></td></tr>
                        <tr><th>जिला</th><td id="pvDistrict"></td></tr>
                        <tr><th>तहसील</th><td id="pvTahsil"></td></tr>
                        <tr><th>गांव</th><td id="pvVillage"></td></tr>
                        <tr><th>पिनकोड</th><td id="pvPincode"></td></tr>
                        <tr><th>पूरा पता</th><td id="pvAddress"></td></tr>
                    </table>

                    <!-- LAND DETAILS -->
                    <h6 class="text-success border-bottom pb-1 mt-3">भूमि विवरण</h6>
                    <table class="table table-sm table-bordered" id="previewLandTable">
                        <thead class="table-light">
                            <tr>
                                <th>तहसील</th>
                                <th>गांव</th>
                                <th>खसरा</th>
                                <th>कुल रकबा</th>
                                <th>हिस्सेदारी</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>

                </div>

                <div class="modal-footer">
                    <button class="btn btn-secondary" data-bs-dismiss="modal">✏ Edit</button>
                    <button type="submit" class="btn btn-success" onclick="SendOTPtoValidate()">
                        ✔ Confirm & Submit
                    </button>
                </div>

            </div>
        </div>
    </div>

    <!-- OTP Modal -->
    <div class="modal fade" id="otpModal" tabindex="-1">
        <div class="modal-dialog modal-dialog-centered modal-sm">
            <div class="modal-content">

                <div class="modal-header border-0 justify-content-center">
                    <h5 class="modal-title">OTP सत्यापन</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
                </div>

                <div class="modal-body text-center">
                    <p class="text-muted">आपके मोबाइल नंबर पर 6 अंकों का OTP भेजा गया है</p>

                    <!-- 6 OTP Inputs -->
                    <div class="otp-boxes">
                        <input type="text" maxlength="1" class="otp-input" />
                        <input type="text" maxlength="1" class="otp-input" />
                        <input type="text" maxlength="1" class="otp-input" />
                        <input type="text" maxlength="1" class="otp-input" />
                        <input type="text" maxlength="1" class="otp-input" />
                        <input type="text" maxlength="1" class="otp-input" />
                    </div>


                    <div id="otp-message" class="mt-2 text-danger"></div>


                    <div class="mt-3">
                        <button class="btn btn-success btn-block" onclick="VerifyOTP()">सत्यापित करें</button>
                    </div>

                    <div class="mt-2 otp-resend">
                        <span>OTP नहीं मिला?</span>
                        <a href="#" onclick="ResendOTP()">फिर से भेजें</a>
                        <span id="otp-timer">30s</span>
                    </div>
                </div>

            </div>
        </div>
    </div>




    <link href="~/toastr/toastr.css" rel="stylesheet" />
    <link href="~/toastr/toastr.min.css" rel="stylesheet" />
    <link href="~/toastr/toastr.js.map" />
    <script src="~/toastr/toastr.min.js"></script>
    <script src="~/toastr/toastr.js"></script>
    <script src="~/toastr/toastr.min.js"></script>

    <script src="@Url.Content("~/Page_Js/farmerregistration.js")"></script>

    <!-- Set TempData as a global JS variable -->
    @if (TempData["Message"] != null)
    {
        <script>
            window.TempMessage = @Html.Raw(Json.Serialize(TempData["Message"]));
        </script>
    }

</body>
</html>
